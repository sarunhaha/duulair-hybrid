{
  "name": "duulair-hybrid",
  "version": "1.0.0",
  "scripts": {
    "dev": "nodemon --exec ts-node src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "test": "jest",
    "test:agents": "jest src/agents --coverage",
    "test:coverage": "jest --coverage",
    "db:migrate": "supabase db push",
    "db:reset": "supabase db reset",
    "agent:validate": "ts-node scripts/validate-specs.ts",
    "agent:deploy": "ts-node scripts/deploy-agents.ts",
    "validate:liff": "node scripts/validate-liff-pages.js",
    "predeploy": "npm run validate:liff",
    "lint": "eslint src/**/*.ts",
    "format": "prettier --write src/**/*.ts",
    "dev-agent": "ts-node src/agents/development/DevAgentCLI.ts",
    "dev-agent:interactive": "ts-node src/agents/development/DevAgentCLI.ts interactive",
    "review": "npm run dev-agent review",
    "review:all": "npm run dev-agent review-all",
    "test:gen": "npm run dev-agent test",
    "doc:gen": "npm run dev-agent doc",
    "debug:agent": "npm run dev-agent debug",
    "dev:implement": "npm run dev-agent dev",
    "pm": "npm run dev-agent pm",
    "vibe": "npm run dev-agent vibe"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.17.0",
    "@line/bot-sdk": "^8.4.1",
    "@supabase/supabase-js": "^2.39.0",
    "@types/qrcode": "^1.5.5",
    "axios": "^1.6.0",
    "commander": "^11.1.0",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "glob": "^10.4.5",
    "node-cron": "^4.2.1",
    "qrcode": "^1.5.4",
    "uuid": "^9.0.1",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.10.0",
    "@types/node-cron": "^3.0.11",
    "@types/uuid": "^9.0.7",
    "jest": "^29.7.0",
    "nodemon": "^3.0.2",
    "prettier": "^3.1.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.3.0"
  }
}
